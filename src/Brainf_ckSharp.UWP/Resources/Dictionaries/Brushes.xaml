<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--Shell header background brush-->
    <AcrylicBrush
        x:Name="ShellHeaderBackgroundBrush"
        BackgroundSource="Backdrop"
        TintColor="#FF1E1E1E"
        TintOpacity="0.6"/>

    <!--Virtual keyboard background-->
    <AcrylicBrush
        x:Name="VirtualKeyboardBackgroundBrush"
        BackgroundSource="HostBackdrop"
        TintColor="#FF101010"
        TintOpacity="0.9"/>

    <!--Backgorund info bar brush-->
    <SolidColorBrush x:Key="BackgroundInfoBrush" Color="#FF007ACC"/>

</ResourceDictionary>
